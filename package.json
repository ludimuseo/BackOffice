{
  "name": "backoffice-dedale",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production tsc -b && vite build --mode production",
    "lint": "eslint .",
    "style": "stylelint src/**/*.{css,scss}",
    "format": "prettier --ignore-unknown --check .",
    "fix": "yarn lint --fix && yarn format --write && yarn style --fix",
    "commitlint": "commitlint --edit",
    "postinstall": "husky",
    "preview": "vite preview"
  },
  "lint-staged": {
    "src/**/*.{css,scss}": [
      "stylelint --fix"
    ],
    "**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --ignore-unknown --write"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0",
    "redux-persist": "^6.0.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@eslint/plugin-kit": "^0.2.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@xpd/tailwind-3dtransforms": "^1.0.3",
    "autoprefixer": "^10.4.20",
    "commit-prettier": "^1.0.5",
    "cross-env": "^7.0.3",
    "daisyui": "^4.12.14",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "firebase": "^11.0.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "i18next": "^23.16.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "lint-staged": "^15.2.10",
    "notistack": "^3.0.1",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "react-i18next": "^15.1.1",
    "sass": "^1.80.6",
    "sass-embedded": "^1.80.6",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-scss": "^6.9.0",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "zod": "^3.23.8"
  },
  "packageManager": "yarn@4.5.1"
}
